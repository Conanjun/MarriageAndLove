<%@ Page Title="" Language="C#" MasterPageFile="~/admin/AdminMasterPage.master" AutoEventWireup="true" CodeFile="passwd_change.aspx.cs" Inherits="admin_passwd_change" %>

<asp:Content ID="Content1" ContentPlaceHolderID="head" Runat="Server">
</asp:Content>
<asp:Content ID="Content2" ContentPlaceHolderID="ContentPlaceHolder1" Runat="Server">
	<form id="form1" runat="server" class="form-horizontal">
		<fieldset>
			<legend>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; 修改密码</legend>
			<table ID="Table1" runat="server" height="250px" width="500px">
			<tr><td width="200">&nbsp;&nbsp; &nbsp;用户名</td><td width="300">
				<asp:TextBox ID="username" runat="server"></asp:TextBox>
				</td></tr>
			<tr><td width="200">&nbsp;&nbsp;&nbsp;&nbsp; 旧密码</td><td width="300">
				<asp:TextBox ID="old_password" runat="server" TextMode="Password"></asp:TextBox>
				</td></tr>
			<tr><td width="200">&nbsp;&nbsp;&nbsp;&nbsp; 新密码</td><td width="300">
				<asp:TextBox ID="new_password" runat="server" TextMode="Password"></asp:TextBox>
				</td></tr>
			<tr><td width="200">&nbsp;&nbsp;&nbsp;&nbsp; 确认密码</td><td width="300">
				<asp:TextBox ID="new_password_again" runat="server" TextMode="Password"></asp:TextBox>
				</td></tr>
			<tr><td width="200">&nbsp;&nbsp;<asp:Button ID="submit_button" runat="server" OnClick="Button1_Click" Text="确认修改" />
				</td></tr>
			</table>
		</fieldset>
    </form>
</asp:Content>

